    "@types/chai-string": "^1.1.31",
    "@types/lodash": "^4.14.88",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.45",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.4.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.12.1",
    "testdouble": "^3.2.6",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "devDependencies": {}
