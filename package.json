  "license": "UNLICENSED",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn run lint && yarn run compile && yarn run test",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "yarn run unit",
    "unit": "mocha --inspect=0.0.0.0:5858 dist/**/*.spec.js",
    "compile": "tsc -p ./",
    "develop": "yarn run compile -w",
    "start": "node ./dist/index.js",
    "debug": "node --inspect=0.0.0.0:5858 ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PickMyLoad/developer-evaluation"
  },
